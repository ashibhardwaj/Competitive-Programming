#include <bits/stdc++.h>
using namespace std;
 
int main() {
	int t, n, e, o;
	cin>>t;
	while(t--)
	{
		e=0;
		o=0;
		cin>>n;
		int a[n];
		for(int i=0; i<n; i++)
		{
			cin>>a[i];
			if(a[i]%2==0)
				e++;
			else
				o++;
		}
		if(e==n)
		{
			cout<<"NO"<<endl;
		}
		else if (n%2==0 && n==o)
		{
			cout<<"NO"<<endl;
		}
		else
		{
			cout<<"YES"<<endl;
		}
	}
}
