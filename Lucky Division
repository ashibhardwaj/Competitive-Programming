#include<iostream>

using namespace std;
int main()
{
	int n;
	cin>>n;
	if(n>=1 && n<=43)
	{
		if(n%4==0 || n%7==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>43 && n<=47)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>47 && n<=74)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>74 && n<=77)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>77 && n<=444)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0 || n%444==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>444 && n<=447)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0 || n%444==0 || n%447==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>447 && n<=474)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0 || n%444==0 || n%447==0 || n%474==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>474 && n<=477)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0 || n%444==0 || n%447==0 || n%474==0 || n%477==0 )
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>477 && n<=744)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0 || n%444==0 || n%447==0 || n%474==0 || n%477==0 || n%744==0 )
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>744 && n<=747)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0 || n%444==0 || n%447==0 || n%474==0 || n%477==0 || n%744==0 || n%747==0 )
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>747 && n<=774)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0 || n%444==0 || n%447==0 || n%474==0 || n%477==0 || n%744==0 || n%747==0 || n%774==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if(n>774 && n<=999)
	{
		if(n%4==0 || n%7==0 || n%44==0 || n%47==0 || n%74==0 || n%77==0 || n%444==0 || n%447==0 || n%474==0 || n%477==0 || n%744==0 || n%747==0 || n%774==0 || n%777==0)
			cout<<"YES";
		else
			cout<<"NO";
	}
	else if (n==1000)
		cout<<"YES";

	return 0;
}
