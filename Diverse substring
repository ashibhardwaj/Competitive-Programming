#include <bits/stdc++.h>
using namespace std;
int main()
{
  int n, flag=0;
  cin>>n;
  string s;
  cin>>s;
  for(int i=0; i<n-1; i++)
  {
      if(s[i]!=s[i+1])
      {
          cout<<"YES"<<endl;
          cout<<s[i]<<s[i+1];
          flag=1;
      }
      if(flag==1)
      break;
  }
  if(flag==0)
  {
      cout<<"NO"<<endl;
  }
  return 0;
}
