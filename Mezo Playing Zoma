#include <iostream>
using namespace std;
 
int main() {
    // insert code here...
    long long n, a=0, b=0;
    string s;
    cin>>n;
    cin>>s;
    for(long long i=0; i<n; i++)
    {
        if(s[i]=='L')
            a--;
        else
            b++;
    }
    long long ans=b-a+1;
    cout<<ans;
    return 0;
}
