#include<bits/stdc++.h>
using namespace std;
int main()
{
    long n,s;
    cin>>n>>s;
    long a = n-1;
    long b = s-a;
    if(n-1 >= s-(n-1)-1)
    {
        cout<<"NO";
        return 0;
    }
    cout<<"YES"<<endl;
    for(int i = 0; i < n-1; i++)
        cout<<1<<" ";
    cout<<s-a<<endl;
    cout<<n;
    return 0;
}
