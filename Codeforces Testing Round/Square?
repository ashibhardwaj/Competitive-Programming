#include<bits/stdc++.h>
#define ll long long
using namespace std;

int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        ll a, b, c, d;
        cin>>a>>b>>c>>d;
        if((a == c && b + d == a)||(a == d && b + c == a)||(b == c && a + d == b)||(b == d && a + c == b))
          cout<<"Yes"<<endl;
        else
          cout<<"No"<<endl;
    }
    return 0;
}
