#include<iostream>
#include<vector>

using namespace std;

int main()
{
	int n, i, sum1=0, sum2=0;
	cin>>n;
	vector <int> x(n);
	vector <int> y(n);
	for(i=0; i<n; i++)
	cin>>x[i];
	for(i=0; i<n; i++)
	cin>>y[i];
	for(i=0; i<n; i++)
	sum1+=x[i];
	for(i=0; i<n; i++)
	sum2+=y[i];
	if(sum2>sum1)
	cout<<"No";
	else
	cout<<"Yes";
	return 0;
}
